{
  "openapi": "3.0.4",
  "info": {
    "title": "servicebusapi2",
    "version": "1.0"
  },
  "paths": {
    "/Message/publish": {
      "post": {
        "tags": [
          "Message"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Details"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/Details"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/Details"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/Message/schedule": {
      "post": {
        "tags": [
          "Message"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ScheduleRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/ScheduleRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/ScheduleRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/Message/stop": {
      "post": {
        "tags": [
          "Message"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/Message/print": {
      "get": {
        "tags": [
          "Message"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Details": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "nullable": true
          },
          "vmsIntegration": {
            "$ref": "#/components/schemas/NestedDetails"
          },
          "sequenceNumber": {
            "type": "string",
            "nullable": true
          },
          "deviceIds": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "NestedDetails": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "customerId": {
            "type": "string",
            "nullable": true
          },
          "videoIntegrationType": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ScheduleRequest": {
        "type": "object",
        "properties": {
          "details": {
            "$ref": "#/components/schemas/Details"
          },
          "intervalMinutes": {
            "type": "integer",
            "format": "int32"
          },
          "durationHours": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      }
    }
  }
}